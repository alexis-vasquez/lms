{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  },
  "globalDependencies": [
    "$NODE_ENV",
    "$PORT",
    "$JWT_SECRET",
    "$POSTGRES_USER",
    "$POSTGRES_PASSWORD",
    "$POSTGRES_DB",
    "$POSTGRES_PORT"
  ]
}