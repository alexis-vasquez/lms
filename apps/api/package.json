{
  "name": "@romalms/api",
  "version": "0.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "code:fix": "yarn eslint --fix && prettier --write .",
    "code:check": "yarn eslint . && tsc --noEmit && prettier --check .",
    "data:migrate": "npx sequelize db:migrate",
    "data:seed": "npx sequelize db:seed:all",
    "data:init": "yarn data:migrate && yarn data:seed",
    "dev": "bash -c 'while !</dev/tcp/localhost/5432; do sleep 5; done; nodemon src/index.ts'",
    "lint": "eslint .",
    "test": "jest",
    "postinstall": "npx typesync"
  },
  "dependencies": {
    "@apollo/server": "^4.0.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "graphql": "^16.6.0",
    "invariant": "^2.2.4",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.6",
    "supertest": "^6.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-typescript": "^7.18.6",
    "@romalms/database": "*",
    "@types/babel__core": "^7.1.19",
    "@types/babel__preset-env": "^7.9.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/eslint": "^8.4.7",
    "@types/express": "^4.17.13",
    "@types/invariant": "^2.2.35",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/nodemon": "^1.19.2",
    "@types/pg": "^8.6.5",
    "@types/prettier": "^2.7.0",
    "@types/supertest": "^2.0.12",
    "babel-jest": "^29.0.2",
    "eslint": "^8.25.0",
    "eslint-config-custom": "*",
    "jest": "^29.0.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "sequelize-cli": "^6.4.1",
    "ts-node": "^10.9.1",
    "tscpaths": "^0.0.9",
    "typescript": "^4.7.4"
  }
}
